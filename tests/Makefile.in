### Do not edit -- Generated by 'configure --with-whatever' from Makefile.in
### RtAudio tests Makefile - for various flavors of unix and MinGW

PROGRAMS = audioprobe playsaw playraw record duplex testall teststops
RM = /bin/rm
SRC_PATH = ..
INCLUDE = @top_srcdir@
OBJECT_PATH = @object_path@
vpath %.o $(OBJECT_PATH)

OBJECTS	=	@top_srcdir@/RtAudio.o @objects@

CC       = @CXX@
DEFS     = @CPPFLAGS@
CFLAGS   = @CXXFLAGS@
CFLAGS  += -I$(INCLUDE) -I@top_srcdir@/include
LIBRARY  = @LIBS@

%.o : $(SRC_PATH)/%.cpp
	$(CC) $(CFLAGS) $(DEFS) -c $(<) -o $(OBJECT_PATH)/$@

%.o : ../include/%.cpp
	$(CC) $(CFLAGS) $(DEFS) -c $(<) -o $(OBJECT_PATH)/$@

all : $(PROGRAMS)

audioprobe : @srcdir@/audioprobe.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o audioprobe $^ $(LIBRARY)

playsaw : @srcdir@/playsaw.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o playsaw $^ $(LIBRARY)

playraw : @srcdir@/playraw.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o playraw $^ $(LIBRARY)

record : @srcdir@/record.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o record $^ $(LIBRARY)

duplex : @srcdir@/duplex.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o duplex $^ $(LIBRARY)

testall : @srcdir@/testall.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o testall $^ $(LIBRARY)

teststops : @srcdir@/teststops.cpp $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -o teststops $^ $(LIBRARY)

clean : 
	$(RM) -f $(OBJECT_PATH)/*.o
	$(RM) -f $(PROGRAMS)
	$(RM) -f *.raw *~ *.exe
	$(RM) -fR *.dSYM

distclean: clean
	$(RM) -f Makefile

strip : 
	strip $(PROGRAMS)
